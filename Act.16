using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Act._16
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string[] symptoms = new string[5];
            string[] keyword = { "fever", "cough" };
            string[] keyword1 = { "injury", "burn" };

            for (int i = 0; i < symptoms.Length; i++)
            {
                Console.Write($"Enter sympton {i + 1}: ");
                symptoms[i] = Console.ReadLine().ToLower();

                if (keyword.Contains(symptoms[i]))
                {
                    Console.WriteLine("Department: General Medicine");
                }
                else if (keyword1.Contains(symptoms[i]))
                {
                    Console.WriteLine("Department: Emergency");
                }
                else
                {
                    Console.WriteLine("Refer to Specialist");
                }
                Console.WriteLine();
            }
        }
    }
}
